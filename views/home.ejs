<%- include('partials/header'); -%>
<h1>Home</h1>
<div>
  <p> <%= homePage %> </p>
</div>
<div>
  <% totalPosts.forEach(function(post) { %>
      <h2> <%= post.title %> </h2>
      <div class="truncate"> <%= post.body %> </div>
      <a href="/posts/<%= post._id %>" >Read More</a>
  <% });                                  %>
</div>
<%- include('partials/footer'); -%>
